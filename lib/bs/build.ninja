ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name Beikao
src_root_dir = /Users/Mikey/Desktop/test
bsc = /Users/Mikey/Desktop/test/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/Mikey/Desktop/test/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/Mikey/Desktop/test/node_modules/bs-platform/lib/ocaml -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = -I /Users/Mikey/Desktop/test/node_modules/reason-react/lib/ocaml -I /Users/Mikey/Desktop/test/node_modules/bs-director/lib/ocaml
bs_package_dev_includes = 
refmt = /Users/Mikey/Desktop/test/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = -ppx '/Users/Mikey/Desktop/test/node_modules/bs-platform/bin/reactjs_jsx_ppx.exe'
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I .
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build Yo.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/Yo.re
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build Yo.mlast.d : build_deps Yo.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build Yo.cmj $src_root_dir/lib/js/Yo.js Yo.cmi : build_cmj_cmi Yo.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js
build page.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/page.re
build page.mlast.d : build_deps page.mlast
build page.cmj $src_root_dir/lib/js/page.js page.cmi : build_cmj_cmi page.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js
build simpleRoot.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/simpleRoot.re
build simpleRoot.mlast.d : build_deps simpleRoot.mlast
build simpleRoot.cmj $src_root_dir/lib/js/simpleRoot.js simpleRoot.cmi : build_cmj_cmi simpleRoot.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js
build build.ninja : phony  ||  simpleRoot.mlast.d page.mlast.d Yo.mlast.d
